{
  "name": "2miners",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "A browser extension to view the progress of your crypto miners.",
  "homepage_url": "http://extensionizr.com",
  "icons": {
    "34": "./icons/icon-34.png",
    "128": "./icons/icon-128.png"
  },
  "background": {
      "script": ["background.js"]
  },
  "browser_action": {
      "default_popup": "popup.html"
  },
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "https://*/*",
    "https://eth.2miners.com/api/accounts/*"
  ]
}
  