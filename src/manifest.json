{
  "name": "2miners",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "A browser extension to view the progress of your crypto miners.",
  "homepage_url": "https://github.com/luke-buttifant/2Miners-Browser-Extension",
  "icons": {
    "16": "./icons/icon-16.png",
    "32": "./icons/icon-32.png",
    "128": "./icons/icon-128.png"
  },
  "background": {
      "script": ["background.js"]
  },
  "action":{
    "default_popup": "popup.html"
  },
  "permissions":["storage"]
}
  